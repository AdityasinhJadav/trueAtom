{
  "version": 2,
  "buildCommand": "npm run setup && npm run build",
  "builds": [
    { "src": "build/server/index.js", "use": "@vercel/node" },
    { "src": "build/client/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/build/(.*)", "dest": "/build/client/$1" },
    { "src": "/(.*)", "dest": "/build/server/index.js" }
  ]
}


